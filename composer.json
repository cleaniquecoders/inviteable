{
    "name": "cleaniquecoders/inviteable",
    "description": "Built with Laravel Standalone Package Creator",
    "keywords": ["package", "laravel", "invite", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Inviteable\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Inviteable\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "cleaniquecoders/blueprint-macro": "^1.0",
        "illuminate/support": "^5.5|^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*",
        "codedungeon/phpunit-result-printer": "^0.4.4|^0.6.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\Inviteable\\InviteableServiceProvider"
            ],
            "aliases": {
                "Inviteable": "CleaniqueCoders\\Inviteable\\InviteableFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
